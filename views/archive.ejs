<%- include header %>

<ul class="archive">
  <% posts.forEach(function (post, index) { %>
    <% var time = post.time.split(" ")[0] + "-" + post.time.split(" ")[1] %>
    <li><time><%= post.time.split("-")[1] + "-" + post.time.split("-")[2].split(" ")[0] %></time></li>
    <li><a href="/u/<%= post.name %>/<%= time %>/<%= post.title %>"> <%= post.title %> </a></li>

  <% }) %>
</ul>
<%- include footer %>
